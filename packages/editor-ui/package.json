{
  "name": "n8n-editor-ui",
  "version": "0.104.0",
  "description": "Workflow Editor UI for n8n",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Jan Oberhauser",
    "email": "jan@n8n.io"
  },
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8n-io/n8n.git"
  },
  "scripts": {
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "cross-env VUE_APP_URL_BASE_API=http://localhost:5678/ vue-cli-service electron:serve",
    "build": "cross-env VUE_APP_PUBLIC_PATH=\"/%BASE_PATH%/\" vue-cli-service build",
    "dev": "npm run serve",
    "format": "cd ../.. && node_modules/prettier/bin-prettier.js packages/editor-ui/**/**.ts --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "serve": "cross-env VUE_APP_URL_BASE_API=http://localhost:5678/ vue-cli-service serve",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "concurrently": "^6.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "n8n-design-system": "~0.1.0",
    "timeago.js": "^4.0.2",
    "v-click-outside": "^3.1.2",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@beyonk/google-fonts-webpack-plugin": "^1.5.0",
    "@electron-forge/cli": "^6.0.0-beta.60",
    "@electron-forge/maker-deb": "^6.0.0-beta.60",
    "@electron-forge/maker-rpm": "^6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.60",
    "@electron-forge/maker-zip": "^6.0.0-beta.60",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.60",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@types/dateformat": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.13",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.14.40",
    "@types/quill": "^2.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.6",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.0.1",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^6.2.0",
    "dateformat": "^3.0.3",
    "electron": "^14.0.0",
    "element-ui": "~2.13.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.16.0",
    "file-saver": "^2.0.2",
    "flatted": "^2.0.0",
    "jquery": "^3.4.1",
    "jshint": "^2.9.7",
    "jsplumb": "2.15.4",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "n8n-workflow": "~0.66.0",
    "normalize-wheel": "^1.0.1",
    "prismjs": "^1.17.1",
    "quill": "^2.0.0-dev.3",
    "quill-autoformat": "^0.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "string-template-parser": "^1.2.6",
    "style-loader": "^3.2.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^9.2.5",
    "tslint": "^6.1.2",
    "typescript": "~4.3.5",
    "uuid": "^8.3.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-json-pretty": "1.7.1",
    "vue-loader": "^16.5.0",
    "vue-prism-editor": "^0.3.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.14",
    "vue-typed-mixins": "^0.2.0",
    "vue2-touch-events": "^3.2.1",
    "vuex": "^3.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "n8n_editor_ui"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/main.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
